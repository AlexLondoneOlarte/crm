{
  "key": "",
  "update_url": "https://clients2.google.com/service/update2/crx",
  "description": "Descripci√≥n del CRM",
  "version": "4",
  "manifest_version": 3,
  "name": "WhatsApp CRM",
  "short_name": "WhatsApp CRM",
  
  "action": {
    "default_icon": "icon-34.png",
    "default_popup": "popup.html",
    "default_title": "WhatsApp CRM"
  },
  
  "icons": {
    "16": "icon-16.png",
    "48": "icon-48.png", 
    "128": "icon-128.png"
  },
  
  "permissions": ["storage", "tabs"],
  "host_permissions": ["*://*.whatsapp.com/*"],
  
  "content_scripts": [
    {
      "matches": ["*://*.whatsapp.com/*"],
      "js": ["contentScript.js"],
      "run_at": "document_idle"
    }
  ]
}